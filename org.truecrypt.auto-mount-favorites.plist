<plist version="1.0">
  <dict>
    <key>Label</key>
    <string>org.truecrypt.auto-mount-favorites</string>
    <key>RunAtLoad</key>
    <true/>
    <key>ProgramArguments</key>
    <array>
        <string>/Applications/TrueCrypt.app/Contents/MacOS/TrueCrypt</string>
        <string>--auto-mount=favorites</string>
    </array>
  </dict>
</plist>


